<div class="card ">
  <div class="card-body">
    <h5 class="card-title text-center mb-4">Informacion de propiedad <%= @property.identifier %></h5>
   
		<div class="cardSpaceLeft">   
			<p>
			  <strong>Usuario:</strong>
			  <%= @property.user_id %>
			</p>

			<p>
			  <strong>Group:</strong>
			  <%= @property.group_id %>
			</p>
			<p>
			  <strong>Residents:</strong>
			  <%= link_to 'Residentes', property_residents_path(@property) %>
			</p>

			<%= link_to 'Editar', edit_group_property_path(@group, @property) %>  |
			<%= link_to 'Propiedades', group_properties_path(@group) %>

		</div>
	</div>
</div>