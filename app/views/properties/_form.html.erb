<div class="card formCard mt-5 mb-5">
  <div class="card-body">
    <div clas='formContainer'>
      <%= simple_form_for(@property) do |f| %>
        <%= f.error_notification %>
        <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

        <div class="form-inputs">
          <%#= f.association :user %>
          <%= f.input :user_id, label: "Usuario #{@user.id}", :as => :hidden, input_html: {value: @user.id} %>       
          <%= f.input :identifier, label: 'NÃºmero' %>
        </div>

        <div class="form-actions">
          <%= f.button :submit, :class => "btn btn-primary greenColor mb-2 mt-2"  %>

        </div>
      <% end %>
    </div>
  </div>
</div>